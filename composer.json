{
    "name": "openclassrooms/clean-architecture",
    "description": "Clean architecture library",
    "type": "library",
    "keywords": ["design"],
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Romain Kuzniak",
            "email": "romain.kuzniak@openclassrooms.com"
        },
        {
            "name": "OpenClassrooms",
            "email": "technique@openclassrooms.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "OpenClassrooms\\": "src/OpenClassrooms/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenClassrooms\\Tests\\": "tests/OpenClassrooms/"
        }
    },
    "require": {
        "doctrine/annotations": "1.*",
        "openclassrooms/cache": "1.0.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
